#ifndef UNTITLED1_COORDINATE_H
#define UNTITLED1_COORDINATE_H


struct Coordinate {
    uint8_t x;
    uint8_t y;

    bool operator==(const Coordinate &other) const {
        return x == other.x && y == other.y;
    }
};

#endif //UNTITLED1_COORDINATE_H
